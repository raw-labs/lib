// Current ISS location
// http://open-notify.org/Open-Notify-API/ISS-Location-Now/
// http://api.open-notify.org/iss-now.json
//

typealias iss := record(
    timestamp: int,
    iss_position: record(
        longitude: string,
        latitude: string
    ),
    message: string
);

iss_location_now() := {
    read_json[iss]("http://api.open-notify.org/iss-now.json")
}